<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="co.edu.udea.compumovil.gr06_20252.cleancode">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Labs20252Gr06">
        <activity
            android:name="co.edu.udea.compumovil.gr06_20252.cleancode.PersonalDataActivity"
            android:exported="true"
            android:label="@string/personal_title"
            android:theme="@style/Theme.Labs20252Gr06"
            tools:ignore="MissingClass">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="co.edu.udea.compumovil.gr06_20252.cleancode.ContactDataActivity"
            android:exported="false"
            android:label="@string/personal_title"
            android:theme="@style/Theme.Labs20252Gr06"
            tools:ignore="MissingClass" />
    </application>

</manifest>